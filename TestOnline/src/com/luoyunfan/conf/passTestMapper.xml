<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.luoyunfan.dao.IPassTestDao">
	<resultMap id="passTestMap" type="com.luoyunfan.entity.PassTest">
		<result column="stu_zh" property="stuZh" jdbcType="VARCHAR" />
		<!-- jdbcType和javaType只是用于在ResultMapping中选择类型处理器，如果不设置这两个参数，也会推导出对应的 jdbcType和javaType -->
		<result column="paper_name" property="paperName" jdbcType="VARCHAR" />
		<result column="test_score" property="testScore" jdbcType="VARCHAR" />
		<result column="test_time" property="testTime" jdbcType="VARCHAR" />
	</resultMap>
	<select id="findStuTest" resultMap="passTestMap">
		select * from alreadytest where stu_zh = #{arg0} 
	</select> 
	<insert id="addHistoryInfo">
		insert into alreadytest (stu_zh,paper_name,test_score,test_time)
		values(#{arg0},#{arg1},#{arg2},#{arg3})
	</insert>
	<!-- <select id="searchAllTest" resultMap="testMap">
		select * from examplan
	</select>
	<delete id="delTest">
		delete from examplan where stu_zh = #{arg0} and paper_name=#{arg1}
	</delete> -->
</mapper>
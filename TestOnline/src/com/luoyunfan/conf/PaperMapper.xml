<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.luoyunfan.dao.IPaperDao">
	<resultMap id="paperMap" type="com.luoyunfan.entity.Paper">
		<id column="paper_name" property="paperName" jdbcType="VARCHAR" />

		<!-- jdbcType和javaType只是用于在ResultMapping中选择类型处理器，如果不设置这两个参数，也会推导出对应的 jdbcType和javaType -->
		<result column="paper_timuid" property="paperTimu" jdbcType="VARCHAR" />
	</resultMap>
	<select id="findPaper" resultMap="paperMap">
		select * from paper where paper_name = #{paperName}
	</select>
	<insert id="addPaper"><!--  useGeneratedKeys="true" keyProperty="stuID" -->
		<selectKey keyProperty="paperName" order="AFTER" resultType="java.lang.String">
			SELECT LAST_INSERT_ID()
		</selectKey>
		insert into paper (paper_name,paper_timuid)
			values(#{arg0},#{arg1})
	</insert>
	<select id="paperSearchName" resultMap="paperMap">
		select *from paper
	</select>
	<delete id="delPaper">
		delete from paper where paper_name = #{paperName}
	</delete>
</mapper>
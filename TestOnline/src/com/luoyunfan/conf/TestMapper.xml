<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.luoyunfan.dao.ITestDao">
	<resultMap id="testMap" type="com.luoyunfan.entity.TestPlan">
		<id column="stu_zh" property="stuZh" jdbcType="VARCHAR" />
		<!-- jdbcType和javaType只是用于在ResultMapping中选择类型处理器，如果不设置这两个参数，也会推导出对应的 jdbcType和javaType -->
		<result column="paper_name" property="paperName" jdbcType="VARCHAR" />
		<result column="end_time" property="endTime" jdbcType="VARCHAR" />
	</resultMap>
	<select id="findTestPlan" resultMap="testMap">
		select * from examplan where stu_zh = #{arg0} and paper_name= #{arg1}
	</select>
	<insert id="addTestPlan">
		insert into examplan (stu_zh,paper_name,end_time)
		values(#{arg0},#{arg1},#{arg2})
	</insert>
	<select id="searchAllTest" resultMap="testMap">
		select * from examplan
	</select>
	<delete id="delTest">
		delete from examplan where stu_zh = #{arg0} and paper_name=#{arg1}
	</delete>
	<update id="changeTime">
		update examplan set end_time = #{endTime}  WHERE stu_zh = #{stuZh} AND paper_name = #{paperName}
	</update>
</mapper>